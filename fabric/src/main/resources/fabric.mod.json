{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "${repo}",
    "sources": "${repo}",
    "issues": "${repo}/issues"
  },
  "license": "${license}",
  "icon": "assets/invmove/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.pieking1215.invmove.fabric.ExampleModFabric"
    ],
    "client": [
      "me.pieking1215.invmove.fabric.client.ExampleModFabricClient"
    ]
  },
  "mixins": [
    "invmove.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.7",
    "minecraft": "~1.21",
    "cloth-config": ">=${cloth_major}"
  }
}
